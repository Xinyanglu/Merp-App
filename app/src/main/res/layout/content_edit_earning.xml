    <fragment
        android:id="@+id/entryFragment"
        android:name="com.merp.android.activities.EntryFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_entry" />
</LinearLayout>